{
  "rules": {
    "indentation": 0,
    "empty-line-between-blocks": 0,
    "no-color-literals": 1,
    "nesting-depth": [
      0,
      { "max-depth": 3 }
    ],
    "mixin-name-format": 0,
    "placeholder-name-format": 0,
    "class-name-format": 0,
    "space-around-operator": 0,
    "brace-style": 0,
    "no-misspelled-properties": [
      1,
      {
        "extra-properties": [
          "font-smoothing"
        ]
      }
    ],
    "no-css-comments": 0,
    "no-vendor-prefixes": 0
  }
}