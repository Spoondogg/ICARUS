<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1.0" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="apple-mobile-web-app-title" content="spoonMEDIA">
    <meta name="description" content="Icarus Web Engine by spoonMedia" />
    <meta name="author" content="Ryan Dunphy" />
    <title>@ViewBag.Title - ICARUS 0.2.2018.02.11</title>
    @* Preload these scripts *@
    @Styles.Render("~/Content/css/bootstrap")
    @Styles.Render("~/Content/css/icarus")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/icarus")
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet">
    <link rel="icon" href="@Url.Content("~/Content/favicon.ico")" />
    <link rel="apple-touch-icon" href="@Url.Content("~/Content/Images/Logo.png")" />
    <link rel="apple-touch-startup-image" href="@Url.Content("~/Content/Images/Logo.png")" />
</head>
<body>
    <script type="text/javascript">
        console.log("Hello, @User.Identity.Name");
    </script>
    @RenderBody()
    <noscript>
        <a href="/noScript">JavaScript must be enabled for this page to function.  Click here for details</a>
    </noscript>
    @RenderSection("scripts", required: false)
    @RenderSection("viewInit", required: false)
</body>
</html>
